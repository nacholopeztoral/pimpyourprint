<div class="navbar-wagon">
  <nav class="topnav">
    <a href="#" onclick="openNav()">
      <svg width="30" height="30" id="icoOpen">
        <path d="M0,5 30,5" stroke="#113140e8" stroke-width="5"/>
        <path d="M0,14 30,14" stroke="#113140e8" stroke-width="5"/>
        <path d="M0,23 30,23" stroke="#113140e8" stroke-width="5"/>
      </svg>
    </a>
  </nav>
  <div class="justify_logo">
    <%= link_to root_path, class: "navbar-wagon-brand" do %>
      <%= image_tag "Logo4.png", height: 50 %>
    <% end %>
  </div>

  <div class="navbar-wagon-right hidden-xs hidden-sm">
    <% unless user_signed_in? %>
      <!-- Login link (when logged out) -->
      <!-- Dropdown list appearing on mobile only -->
      <%= link_to "About", about_path, class: "navbar-wagon-item navbar-wagon-link" %>
      <%= link_to t(".sign_in", default: "Login"), new_user_session_path, class: "navbar-wagon-item navbar-wagon-link" %>
    <% end %>
  </div>
</div>
